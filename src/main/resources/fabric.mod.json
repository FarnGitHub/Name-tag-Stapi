{
  "schemaVersion": 1,
  "id": "farnnametag",
  "version": "${version}",

  "name": "Farn's Name Tag",
  "description": "Add Name tag to the game (doesn't work the same as modern minecraft though)",
  "authors": [
    "farnfarn02"
  ],

  "license": "MIT",
  "icon": "assets/farnnametag/icon.png",
  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "farn.nametag.listener.NameTagStationAPI"
    ],
    "gcapi3": [
      "farn.nametag.listener.NameTagGlassConfig"
    ],
    "bhcreative:event_bus": [
      "farn.nametag.listener.NameTagBHCreative"
    ]
  },
  "mixins": [
    "farnnametag.mixins.json"
  ],
  "classTweaker": "farnnametag.classTweaker",
  "depends": {
    "minecraft": "1.0.0-beta.7.3",
    "gcapi3": "*"
  }
}
